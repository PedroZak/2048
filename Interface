import java.awt.*;
import javax.swing.*;

public class Interface extends JFrame{
    
    Screen s;
    
    public Interface(){
        setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        setSize(366,420);
        setResizable(false);
        setTitle("Teste");
        
        init();
    }
    
    public void init(){
        setLocationRelativeTo(null);
        
        setLayout(new GridLayout(1,1,0,0));
        
        s= new Screen();
        add(s);
        
        setVisible(true);     
    }

    public static void main(String[] args) {
        new Interface();
    }
}
